public class Concu{

    public static void main(String[] args){
	Hilo h = new Hilo();
	h.start();
	try {
	    Thread.sleep(10);
	}
	catch(Exception e){}
	System.out.println("Adios Mundo");
    }

    class Hilo extends Thread {
	public void run(){
	    System.out.println("Hola mundo");
	}
    }    
}

